---
  interface Props {
    title:string
  }
  const { title } = Astro.props as Props
---

<article class="border border-gray/15 flex flex-col gap-5 p-6 rounded-xl">
  <h3 class="text-xl font-medium">{title}</h3>
  <img class="w-full" src="/line.svg" alt="line">
  <p>
    <slot/>
  </p>  
</article>

